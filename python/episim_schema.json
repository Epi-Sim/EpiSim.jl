{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EpiSim Configuration Schema",
  "type": "object",
  "required": [
    "epidemic_params",
    "population_params",
    "simulation",
    "data"
  ],
  "properties": {
    "simulation": {
      "type": "object",
      "required": [
        "engine",
        "start_date",
        "end_date"
      ],
      "properties": {
        "engine": {
          "type": "string",
          "enum": [
            "MMCACovid19",
            "MMCACovid19Vac"
          ]
        },
        "start_date": {
          "type": "string",
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        },
        "end_date": {
          "type": "string",
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        },
        "save_full_output": {
          "type": "boolean"
        },
        "save_observables": {
          "type": "boolean"
        },
        "save_time_step": {
          "type": [
            "integer",
            "null"
          ],
          "minimum": -1
        },
        "output_folder": {
          "type": "string"
        },
        "output_format": {
          "type": "string",
          "enum": [
            "netcdf",
            "csv",
            "hdf5"
          ]
        },
        "input_format": {
          "type": "string",
          "enum": [
            "csv",
            "netcdf"
          ]
        }
      },
      "additionalProperties": false
    },
    "data": {
      "type": "object",
      "required": [
        "initial_condition_filename",
        "metapopulation_data_filename",
        "mobility_matrix_filename"
      ],
      "properties": {
        "initial_condition_filename": {
          "type": "string"
        },
        "metapopulation_data_filename": {
          "type": "string"
        },
        "mobility_matrix_filename": {
          "type": "string"
        },
        "kappa0_filename": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
